<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/rl_layout"
		android:layout_width="fill_parent"
		android:layout_height="@dimen/head_height"
		android:background="@color/white"
		android:gravity="center_vertical"  > 
       <Button
           android:id="@+id/head_btn_back"
           android:layout_width="@dimen/head_btn_width"
           android:layout_height="@dimen/head_btn_height"
           android:layout_centerVertical="true"
           android:gravity="center_vertical"
           android:text="@string/ret"
           android:textSize="@dimen/head_btn_textsize"
           android:textColor="@color/black"
           android:background="@drawable/title_btn_back"
	    />    
     	<TextView
     	    android:id="@+id/head_ret_textview"
	   		android:layout_width="wrap_content" 
	   		android:layout_height="wrap_content" 
	   		android:text="@string/head"
	   		android:layout_centerInParent="true"
	   		android:textSize="@dimen/head_textsize"		
			android:textColor="@color/black" /> 
   
</RelativeLayout>

